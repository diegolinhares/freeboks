<% content_for :header do %>
  <%= render "web/librarians/shared/document_header",
    current: "Borrowings",
    title: "Borrowings",
    description: "Members with overdue books."
  %>
<% end %>

<section>
  <% if members.present? %>
    <%= render partial: "web/librarians/borrowings/members",
               locals: { members:, pagy: }
    %>
  <% else %>
    <hgroup>
      <p>No members.</p>
    </hgroup>
  <% end %>
</section>
